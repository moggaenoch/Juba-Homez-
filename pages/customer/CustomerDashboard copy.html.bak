<!-- =========================
search.html (Public + logged-in)
========================= -->
<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width,initial-scale=1" />
  <title>Search Listings</title>
</head>
<body>
  <header>
    <h1>Search Listings</h1>
    <nav aria-label="Primary">
      <a href="index.html">Home</a> |
      <a href="search.html">Search</a> |
      <a href="agents.html">Brokers</a> |
      <a href="photographers.html">Photographers</a> |
      <a href="auth-login.html">Login</a>
    </nav>
  </header>

  <main>
    <section aria-labelledby="filters">
      <h2 id="filters">Filters</h2>

      <form action="search.html" method="get">
        <fieldset>
          <legend>Find properties</legend>

          <label for="q">Keyword</label>
          <input id="q" name="q" type="search" placeholder="e.g., 3 bedroom, near airport" />

          <label for="purpose">Purpose</label>
          <select id="purpose" name="purpose">
            <option value="">Any</option>
            <option value="rent">Rent</option>
            <option value="buy">Buy</option>
          </select>

          <label for="type">Property type</label>
          <select id="type" name="type">
            <option value="">Any</option>
            <option>House</option>
            <option>Apartment</option>
            <option>Land</option>
            <option>Commercial</option>
          </select>

          <label for="location">Location</label>
          <input id="location" name="location" type="text" placeholder="e.g., Juba, Munuki" />

          <label for="beds">Bedrooms</label>
          <select id="beds" name="beds">
            <option value="">Any</option>
            <option>Studio</option>
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4+</option>
          </select>

          <label for="min">Min price</label>
          <input id="min" name="min" type="number" min="0" placeholder="0" />

          <label for="max">Max price</label>
          <input id="max" name="max" type="number" min="0" placeholder="5000" />

          <label for="sort">Sort</label>
          <select id="sort" name="sort">
            <option value="newest">Newest</option>
            <option value="price_low">Price (low to high)</option>
            <option value="price_high">Price (high to low)</option>
            <option value="popular">Most viewed</option>
          </select>

          <button type="submit">Search</button>
          <button type="reset">Reset</button>
        </fieldset>
      </form>
    </section>

    <hr />

    <section aria-labelledby="results">
      <h2 id="results">Results</h2>

      <p><strong>Map placeholder:</strong> (integrate Google Maps / Mapbox later)</p>

      <ul>
        <li>
          <article>
            <h3><a href="listing.html?id=201">Listing #201 — 2 Bed Apartment</a></h3>
            <p>Location: Juba | Price: 1,200 | Beds: 2 | Baths: 1</p>
            <p><a href="listing.html?id=201">View details</a></p>
          </article>
        </li>
        <li>
          <article>
            <h3><a href="listing.html?id=202">Listing #202 — 3 Bed House</a></h3>
            <p>Location: Juba | Price: 2,500 | Beds: 3 | Baths: 2</p>
            <p><a href="listing.html?id=202">View details</a></p>
          </article>
        </li>
        <li>
          <article>
            <h3><a href="listing.html?id=203">Listing #203 — Land Plot</a></h3>
            <p>Location: Juba | Price: 10,000 | Size: 50x100</p>
            <p><a href="listing.html?id=203">View details</a></p>
          </article>
        </li>
      </ul>

      <nav aria-label="Pagination">
        <a href="#">Prev</a> |
        <a href="#">1</a> |
        <a href="#">2</a> |
        <a href="#">Next</a>
      </nav>
    </section>
  </main>

  <footer>
    <a href="terms.html">Terms</a> | <a href="privacy.html">Privacy</a>
  </footer>
</body>
</html>


