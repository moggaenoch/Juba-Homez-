    <style>
        /* Specific styling for the Property Detail Page */
        .property-container {
            max-width: 1000px;
            margin: 0 auto;
            padding: 20px;
            background-color: white;
        }

        /* Header section with Title and Price */
        .prop-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            border-bottom: 2px solid #eee;
            padding-bottom: 15px;
            margin-bottom: 20px;
            flex-wrap: wrap;
        }
        
        .prop-header h1 {
            margin: 0;
            font-size: 2rem;
            color: #333;
        }

        .prop-price {
            font-size: 1.5rem;
            color: #d9534f; /* A nice highlight color */
            font-weight: bold;
        }

        /* Main Image */
        .main-image {
            width: 100%;
            height: 500px; /* Fixed height for consistency */
            object-fit: cover; /* Ensures image covers area without stretching */
            border-radius: 8px;
            margin-bottom: 20px;
            border: 1px solid #ddd;
        }

        /* Key Details Bar (Beds, Baths, etc.) */
        .key-details {
            display: flex;
            justify-content: space-around;
            background-color: #f9f9f9;
            padding: 20px;
            border-radius: 8px;
            margin-bottom: 30px;
            border: 1px solid #ddd;
        }

        .detail-item {
            text-align: center;
        }

        .detail-item strong {
            display: block;
            font-size: 1.2rem;
            color: #333;
        }

        .detail-item span {
            color: #666;
            font-size: 0.9rem;
        }

        /* Description and Features Layout */
        .content-grid {
            display: grid;
            grid-template-columns: 2fr 1fr; /* Description takes 2/3, Features takes 1/3 */
            gap: 40px;
            margin-bottom: 40px;
        }

        .features-list ul {
            list-style: none;
            padding: 0;
        }

        .features-list li {
            padding: 10px 0;
            border-bottom: 1px solid #eee;
        }

        /* Contact Button */
        .contact-section {
            text-align: center;
            padding: 40px;
            background-color: #333;
            color: white;
            border-radius: 8px;
            margin-top: 20px;
        }

        .contact-btn {
            background-color: white;
            color: #333;
            padding: 15px 30px;
            text-decoration: none;
            font-weight: bold;
            border-radius: 5px;
            transition: background 0.3s;
            display: inline-block;
            margin-top: 10px;
        }

        .contact-btn:hover {
            background-color: #ddd;
        }

        /* Responsive design for mobile */
        @media (max-width: 768px) {
            .content-grid {
                grid-template-columns: 1fr; /* Stack into one column on mobile */
            }
            .main-image {
                height: 300px;
            }
        }

        /* Gallery Container */
.gallery-container {
    width: 100%;
    margin-bottom: 30px;
}

/* Main Large Image Styling */
.main-image {
    width: 100%;
    height: 500px; /* Fixed height keeps the page stable */
    object-fit: cover;
    border-radius: 8px;
    border: 1px solid #ddd;
    transition: opacity 0.3s ease; /* Smooth fade effect */
}

/* Thumbnail Row Container */
.thumbnail-row {
    display: flex;
    gap: 10px;
    margin-top: 10px;
    overflow-x: auto; /* Allows scrolling if you have many photos */
    padding-bottom: 5px;
}

/* Individual Thumbnails */
.thumb {
    width: 100px;  /* Width of small images */
    height: 80px;  /* Height of small images */
    object-fit: cover;
    border-radius: 5px;
    cursor: pointer;
    opacity: 0.6; /* Dimmed by default */
    border: 2px solid transparent;
    transition: 0.3s;
}

/* Hover Effect */
.thumb:hover {
    opacity: 1;
    border-color: #333;
}
    </style>
