    <script>
    function changeImage(element) {
        // 1. Get the main image tag
        var mainImg = document.getElementById("current-img");
        
        // 2. Change the source of the main image to the clicked thumbnail's source
        mainImg.src = element.src;
        
        // Optional: Reset opacity for all thumbs and highlight the active one
        var thumbs = document.querySelectorAll(".thumb");
        thumbs.forEach(t => t.style.opacity = "0.6");
        element.style.opacity = "1";
    }
</script>

